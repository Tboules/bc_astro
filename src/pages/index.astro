---
import "@/styles/globals.css";
import { contentfulClient } from "../lib/contentful";
import type { TypeHomePageSkeleton } from "../types/contentful";
import MainLayout from "@/components/layouts/main-layout.astro";
import ConsoleButt from "@/components/console-butt";

const homePage =
  await contentfulClient.withoutUnresolvableLinks.getEntries<TypeHomePageSkeleton>(
    {
      content_type: "homePage",
    },
  );
---

<MainLayout title="Boules Consulting">
  <main class="overflow-x-hidden">
    <ConsoleButt client:load />
    <p>Poppins is fun</p>
    <pre>
        {JSON.stringify(homePage.items[0], null, 2)}
    </pre>
  </main>
</MainLayout>
